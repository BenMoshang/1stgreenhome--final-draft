{
	"name": "1stgreenhome--final-draft",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"stylelint": "stylelint \"**/*.{css,scss,svelte}\" --ignore-pattern=\"**/node_modules/**\"",
		"stylelint:fix": "stylelint --fix \"**/*.{css,scss,svelte}\""
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@mapbox/stylelint-processor-arbitrary-tags": "^0.4.0",
		"@stylistic/stylelint-plugin": "^3.1.2",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/node": "^22.14.1",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"postcss": "^8.5.3",
		"postcss-html": "^1.8.0",
		"postcss-load-config": "^5.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.2",
		"sass": "^1.69.7",
		"stylelint": "^16.17.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-idiomatic-order": "^10.0.0",
		"stylelint-config-sass-guidelines": "^12.1.0",
		"stylelint-config-standard": "^37.0.0",
		"stylelint-config-standard-scss": "^14.0.0",
		"stylelint-order": "^6.0.4",
		"stylelint-plugin-defensive-css": "^1.0.4",
		"stylelint-plugin-logical-css": "^1.2.2",
		"stylelint-processor-arbitrary-tags": "^0.1.0",
		"stylelint-processor-html": "^1.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^5.1.4",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0"
	},
	"keywords": [
		"svelte"
	],
	"dependencies": {
		"@chainlift/liftkit-css": "^1.1.1",
		"@motionone/dom": "^10.18.0",
		"lenis": "^1.2.3"
	}
}
